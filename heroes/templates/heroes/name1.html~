<script>
$(document).ready(function(){
    $("button").click(function(){
        $.get("send",
        {
          name: "Donald Duck",
          message: "Duckburg"
        },
        function(data,status){
            alert("Data: " + data + "\nStatus: " + status);
        });
    });
});
</script>
